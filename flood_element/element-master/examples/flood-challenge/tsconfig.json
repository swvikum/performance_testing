{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2015",
		"moduleResolution": "node",
		"lib": ["dom", "esnext"],
		"pretty": true,
		"strictNullChecks": false,
		"allowUnreachableCode": false,
		"alwaysStrict": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"allowJs": true,
		"checkJs": true,
		"noImplicitAny": false,
		"allowSyntheticDefaultImports": true
	}
}
